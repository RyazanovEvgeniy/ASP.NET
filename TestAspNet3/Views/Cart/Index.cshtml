@model CartViewModel
<div class="container">
    @{ foreach (var el in Model.cart.cartItems)
    {
        <div class="alert alert-warning mt-3">
            <b>Автомобиль: </b> @el.product.name<br />
            <b>Цена: </b> @el.product.price.ToString("c")<br />
        </div>
    }
    }
    <hr />
    <a class="btn btn-danger" asp-controller="Order" asp-action="CheckOut">Оплатить</a>
</div>