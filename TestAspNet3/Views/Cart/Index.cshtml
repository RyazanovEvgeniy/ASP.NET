@model CartViewModel
<div class="container">
    @{ foreach (var el in Model.cart.cartItems)
    {
        <div class="alert alert-warning mt-3">
            <b>Автомобиль: </b> @el.product.name<br />
            <b>Цена: </b> @el.product.price.ToString("с")<br />
        </div>
    }
    }
    <hr />
    <div class="btn btn-danger" asp-controller="Order">Оплатить</div>
</div>